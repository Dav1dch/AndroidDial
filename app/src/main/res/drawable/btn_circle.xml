<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#efefef" /><!-- 填充的颜色 -->
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <corners android:topLeftRadius="360dp"
        android:topRightRadius="360dp"
        android:bottomRightRadius="360dp"
        android:bottomLeftRadius="360dp"/>
    <!-- 边框粗细及颜色 -->
    <stroke android:width="0dp" android:color="#999999" />
</shape>
